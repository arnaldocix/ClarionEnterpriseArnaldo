#GROUP(%Parametros)
CargaParametros ROUTINE        
        FREE(QHList)
        #SET(%reg2,%false)
        #IF(%EcAutomatic='Manual')
          #FOR(%EvoCampos)
           #SET(%reg2,%reg2 + 1)
           QHL:Id      = %reg2
           #IF(%EvoHeaderF)
           QHL:Nombre  = %EvoHeaderF
           #ELSE
           QHL:Nombre  = '%EvoHeader'
           #END
           QHL:Longitud   = 100
           QHL:Pict             = '%EvoPict'
           QHL:TextColor  = %HHTextColor
           QHL:TextBack   = %HHTextBackground
           QHL:TextFont   = '%HHTextFont'
           QHL:TextFontSize   = %FFTextFontSize
           QHL:FTextColor        = %FFTextColor
           QHL:FTextBack         = %FFTextBackground
           QHL:FTextFont          ='%FFTextFont'
            QHL:FTextFontSize  =%FFTextFontSize
           ADD(QHList)
          #ENDFOR
        #ELSE
          #FOR(%ReportControl)
           #IF(%EcValidaControl() AND %ReportControlUse <> '' AND %EvoEnableControl)
           #SET(%reg2,%reg2 + 1)
           QHL:Id      = %reg2
           QHL:Nombre  = '%EvoHeader2'
           QHL:Longitud= 100
           QHL:Pict    = '%EvoPict2'
           QHL:TextColor  = %HHTextColor
           QHL:TextBack   = %HHTextBackground
           QHL:TextFont   = '%HHTextFont'
           QHL:TextFontSize   = %FFTextFontSize
           QHL:FTextColor        = %FFTextColor
           QHL:FTextBack         = %FFTextBackground
           QHL:FTextFont          ='%FFTextFont'
            QHL:FTextFontSize  =%FFTextFontSize
           ADD(QHList)
           #ENDIF
          #ENDFOR
        #ENDIF
        !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
        FREE(QPar)
        #INSERT(%ExManejoCampos)
        Q:FieldPar  = '%lista'
        ADD(QPar)!.1.
        Q:FieldPar  = '%SeparatorASCII'
        ADD(QPar)!.2.
        Q:FieldPar  = '%EcLenguaje'
        ADD(QPar)!.3.
        #IF (Not %PathFiles)
        Q:FieldPar  = ''
        #ELSE
        Q:FieldPar  = %PathFiles
        #END
        ADD(QPar)!.4.
        #IF (Not %DisablePreview)
        Q:FieldPar  = true
        #ELSE
        Q:FieldPar  = false
        #END
        ADD(QPar)!.5.
        #IF (Not %EcFileName)
        Q:FieldPar  = ''
        #ELSE
        Q:FieldPar  = %EcFileName
        #END
        ADD(QPar)!.6.
        #IF(%reg2)
        Q:FieldPar  = %reg2
        #ELSE
        Q:FieldPar  = ''
        #END
        ADD(QPar)!.7.
        Q:FieldPar  = 'FREE'
        ADD(QPar)   !.8.
        #IF(%TituloEx1)
        Titulo = %TituloEx1
        #ELSIF (%TituloEx)
        Titulo = '%TituloEx'
        #ENDIF
        #IF(UPPER(%ProcedureTemplate) = 'UNIVABCREPORT')
        Q:FieldPar  = 'NOPREVIEW'
        #ELSIF(%EcNoPreview=1)
        Q:FieldPar  = 'NOPREVIEW'
        #ELSE
        Q:FieldPar  = 'REPORT'
        #ENDIF
        ADD(QPar)   !.9.
        Q:FieldPar  = 1 !Order
         ADD(QPar)   !.10
        #IF (%LineFrom<>'')
         Q:FieldPar  = %LineFrom
         #ELSE
         Q:FieldPar  = 0
         #END
         ADD(QPar)   !.11 
         Q:FieldPar  = '%CreateFileExcel'
         ADD(QPar)   !.12 

         #IF (%PathImage<>'')
         Q:FieldPar  = %PathImage
         #ELSE
         Q:FieldPar  = ''
         #END
         ADD(QPar)   !.13
 
         #IF (%EcHeader1<>'')
         Q:FieldPar  = %EcHeader1
         #ELSE
         Q:FieldPar  = ''
         #END
         ADD(QPar)   !.14

         #IF (%EcHeader2<>'')
         Q:FieldPar  = %EcHeader2
         #ELSE
         Q:FieldPar  = ''
         #END
         ADD(QPar)   !.15
      
         Q:FieldPar  = '%ECFormato'
        ADD(QPar)   !.16

        Q:FieldPar  =  %TTextColor
        ADD(QPar)   !.17.
        Q:FieldPar  =  %TTextBackground
        ADD(QPar)   !.18.
        Q:FieldPar  =  '%TTextFont'
        ADD(QPar)   !.19.
        Q:FieldPar  =  %TTextFontSize
        ADD(QPar)   !.20.
        Q:FieldPar  = 0 !Exporta a excel sin borrar
        ADD(QPar)     !.21
        Q:FieldPar  = 0         !Nro Pag. Desde Report (BExp)
        ADD(QPar)     !.22
        Q:FieldPar  = 0         
        ADD(QPar)     !.23 Caracteres Encoding para xml
 #!IF (%EcUseOO<>'')
 #!     Q:FieldPar   = %EcUseOO
 #!ELSE
        Q:FieldPar  = 1
 #!END
        ADD(QPar)      !24
        Q:FieldPar  = '13021968'       
        ADD(QPar)     !.25
        !.26 en adelante
        #FOR(%EcCampos)
        #IF(%EcCampoTipo = 'Footer')
        Q:FieldPar  = '(F)'&%EcCampo
        #ELSE
        Q:FieldPar  = '(H)'&%EcCampo
        #END
        ADD(QPar)     
        #ENDFOR
        #!FOR(%EcGraficos)
        #!Q:FieldPar  = '(G)%TipoGrafica*'&%EcNameGraf
        #!ADD(QPar)     !.22 en adelante
        #!ENDFOR
!!! Parametros Grupo
